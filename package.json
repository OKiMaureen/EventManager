{
  "name": "eventmanager",
  "version": "1.0.0",
  "description": "Eventmanager is an Andela Bootcamp cycle 29 challenge  app that will help you accept applications to use your center / facilities, and will either decline events when the proposed day is already taken, or suggest an available day",
  "main": "index.js",
  "dependencies": {
    "babel-watch": "^2.0.7",
    "bootstrap": "^4.0.0-alpha.6",
    "chai-http": "^3.0.0",
    "mocha": "^4.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.6.0"
  },
  "scripts": {
    "start": "nodemon --watch server --exec babel-node server/app.js",
    "test": " nyc --reporter=html --reporter=text mocha ./server/test/ --exit --compilers js:babel-register",
    "coveralls": "jscoverage lib && YOURPACKAGE_COVERAGE=1 nodeunit --reporter=lcov test | coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "name": "node ./server/lib/controller/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OKiMaureen/EventManager.git"
  },
